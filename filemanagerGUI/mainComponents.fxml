                     </HBox.margin>
                  </Button>
                  <Button fx:id="buttonForw" mnemonicParsing="false" onAction="#changeToForward" text="--&gt;">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </Button>
                  <TextField fx:id="currentDirText" prefWidth="353.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
