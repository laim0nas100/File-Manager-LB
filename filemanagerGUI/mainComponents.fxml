   <left>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" wrappingWidth="45.13671875" BorderPane.alignment="CENTER" />
   </left>
   <bottom>
      <GridPane BorderPane.alignment="CENTER">
        <columnConstraints>
